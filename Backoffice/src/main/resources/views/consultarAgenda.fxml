<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.clinicadesktop.controllers.ConsultarAgendaController">
    <children>
        <VBox spacing="10.0" alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" prefWidth="600.0" prefHeight="400.0">
            <Label text="Consultar Agenda de Veterinário" style="-fx-font-size: 18px; -fx-font-weight: bold;" />

            <ComboBox fx:id="veterinarioComboBox" prefWidth="300.0" promptText="Escolha o veterinário"/>

            <TableView fx:id="consultaTable" prefHeight="300.0" prefWidth="580.0">
                <columns>
                    <TableColumn fx:id="colData" text="Data" />
                    <TableColumn fx:id="colAnimal" text="Animal" />
                </columns>
            </TableView>
        </VBox>
    </children>
</AnchorPane>
